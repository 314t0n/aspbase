@model Piton.WebUI.Models.ProductListViewModel

@{
    ViewBag.Title = "Products";
}

<h2>List</h2>

@foreach (var p in Model.Products)
{
   @Html.Partial("ProductSummary", p);
}
<ul class="pagination">

    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new {page = x}), "active")

</ul>
